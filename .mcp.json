{
    "mcpServers": {
        "sequential-thinking": {
            "command": "mcp-server-sequential-thinking",
            "args": [
                "--max-thoughts=4",
                "--branch-limit=1",
                "--detail-level=minimal",
                "--revision-depth=1"
            ],
            "env": {
                "DISABLE_THOUGHT_LOGGING": "false"
            },
            "startupDelay": 400,
            "priority": "optional",
            "timeout": 15000
        }
    }
}